{
  "name": "linktree-clone",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "./setup.sh",
    "start": "node dist/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "build": "./build.sh",
    "clean": "rm -rf dist/",
    "rebuild": "npm run clean && npm run build",
    "docker:build": "docker build -t linktree-clone .",
    "docker:run": "docker run -p 3000:3000 linktree-clone",
    "docker:publish": "./publish-docker.sh",
    "prestart": "[ -d \"dist\" ] || npm run build",
    "prepare": "npm run setup"
  },
  "keywords": [
    "linktree",
    "links",
    "express",
    "typescript",
    "sqlite",
    "docker",
    "portfolio",
    "social-media"
  ],
  "author": "Linktree Clone Developer",
  "license": "MIT",
  "description": "Uma aplicação web que imita a funcionalidade do Linktree, permitindo criar e gerenciar links em uma única página.",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/linktree"
  },
  "bugs": {
    "url": "https://github.com/yourusername/linktree/issues"
  },
  "homepage": "https://github.com/yourusername/linktree",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.1",
    "@types/express-session": "^1.18.1",
    "@types/method-override": "^3.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.18",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
